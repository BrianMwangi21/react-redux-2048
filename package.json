{
  "name": "react-redux-starter",
  "version": "0.0.1",
  "description": "Starter Kit for React + Redux application with Webpack",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "webpack --config webpack.config.production.js --colors --bail",
    "start": "node server.js",
    "start:debug": "DEBUG=true npm start",
    "test": "istanbul cover _mocha -- --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "test:lint": "eslint test",
    "lint": "eslint src/scripts",
    "build": "rm build/*; NODE_ENV=production webpack --config webpack.config.production.js --colors --bail"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "starter",
    "babel",
    "hot",
    "eslint",
    "istanbul",
    "mocha",
    "chai"
  ],
  "author": "Dimitar Valchanov",
  "license": "MIT",
  "dependencies": {
    "babel": "~5.8.29",
    "history": "^1.13.1",
    "immutable": "^3.7.5",
    "keymirror": "^0.1.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.1",
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.6",
    "babel-loader": "~5.3.3",
    "babel-runtime": "~5.8.29",
    "chai": "^3.4.1",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-strip-comments": "^1.0.1",
    "html-webpack-plugin": "^1.7.0",
    "istanbul": "^0.3.22",
    "jsdom": "^7.1.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash": "^3.10.1",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
